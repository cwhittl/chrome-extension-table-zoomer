{
    "manifest_version": 3,
    "name": "Datagrid Zoomer",
    "description": "A minimal plugin used to allow zooming into datagrids",
    "version": "1.0.0",
    "icons": {
        "16": "logo/logo-16.png",
        "48": "logo/logo-48.png",
        "128": "logo/logo-128.png"
    },
    "permissions": [],
    "host_permissions": [],
    "content_scripts": [
        {
            "js": [
                "foreground.js"
            ],
            "matches": [
                "https://*.jbhunt.com/*"
            ]
        }
    ]
}