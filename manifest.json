{
    "manifest_version": 3,
    "name": "Datagrid Zoomer",
    "description": "This plugin is made for J.B. Hunt employees that need additional zooming on datagrids due to issues with their vision.",
    "version": "1.2.0",
    "icons": {
        "16": "logo/logo-16.png",
        "48": "logo/logo-48.png",
        "128": "logo/logo-128.png"
    },
    "content_scripts": [
        {
            "js": [
                "foreground.js"
            ],
            "matches": [
                "https://*.jbhunt.com/*"
            ]
        }
    ]
}